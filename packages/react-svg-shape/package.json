{
  "name": "react-svg-shape",
  "version": "1.0.10",
  "description": "A lightweight React library for generating beautiful SVG blob shapes with customizable complexity, contrast, and styling options.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "rm -f dist/*.js.map",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build && cp ../../README.md ./README.md"
  },
  "dependencies": {
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/d3-array": "^3.2.1",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-shape": "^3.1.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "react",
    "svg",
    "shape",
    "generator",
    "animation"
  ],
  "author": "Krystof",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/krystxf/react-svg-shape"
  },
  "homepage": "https://react-svg-shape.vercel.app/",
  "packageManager": "pnpm@9.15.0"
}
